//图片下面添加说明
$("article p>img").each(function(){
	var alt=$(this).attr("alt");
	if(alt==""){
	   alt="MengXiangJia.info";
	   $(this).attr("alt",alt)
	}
	$(this).attr("title",alt);
	$(this).parent().css({"text-align":"center"});
	$(this).parent().after("<p style='text-align:center;padding:0 1em;margin-top:-1em;'><span style='display:inline-block;border-bottom:1px solid #CCCCCC;font-style:italic;color:#999999;min-width:160px;'>&nbsp;"+alt+"&nbsp;</span></p>");
});
</script>
